/*
==========================================
========================================== 
Breakpoint and Grid Variables
==========================================
==========================================
*/


$viewportIncrement: 1px;

$small: 480px;
$medium: 768px;
$mediumLarge: 992px;
$large: 1200px;

$preMedium: $medium - $viewportIncrement;
$preMediumLarge: $mediumLarge - $viewportIncrement;
$preLarge: $large - $viewportIncrement;


/*
==========================================
========================================== 
xxxx. Font Styles
==========================================
==========================================
*/

@font-face {
	font-family: "amatic_scregular";
	src: url("amaticsc-regular-webfont.eot");
	src: url("amaticsc-regular-webfont.eot?#iefix") format("embedded-opentype"), url("amaticsc-regular-webfont.woff2") format("woff2"), url("amaticsc-regular-webfont.ttf") format("truetype");
	font-weight: normal;
	font-style: normal;}

@font-face {
	font-family: "montserratregular";
	src: url("montserrat-regular-webfont.eot");
	src: url("montserrat-regular-webfont.eot?#iefix") format("embedded-opentype"), url("montserrat-regular-webfont.woff2") format("woff2"), url("montserrat-regular-webfont.ttf") format("truetype");
	font-weight: normal;
	font-style: normal;}

@font-face {
	font-family: "lucida_granderegular";
	src: url("lucidagrande-webfont.eot");
	src: url("lucidagrande-webfont.eot?#iefix") format("embedded-opentype"), url("lucidagrande-webfont.woff2") format("woff2"), url("lucidagrande-webfont.ttf") format("truetype");
	font-weight: normal;
	font-style: normal;}

@font-face {
	font-family: "icomoon-alt";
	src: url("icomoon-alt.eot?d1um05");
	src: url("icomoon-alt.eot?d1um05#iefix") format("embedded-opentype"), url("icomoon-alt.ttf?d1um05") format("truetype"), url("icomoon-alt.woff?d1um05") format("woff"), url("icomoon-alt.svg?d1um05#icomoon-alt") format("svg");
	font-weight: normal;
	font-style: normal;}


$font_A: "amatic_scregular", Menlo, Monaco, Consolas, "Courier New", monospace;
$font_B: "montserratregular", Helvetica, Arial, sans-serif;
$font_C: "lucida_granderegular", Helvetica, Arial, sans-serif;
$font_Icon: "icomoon-alt";
$font_Z: 'HelveticaNeue', 'Helvetica Neue', Helvetica, Arial, sans-serif;



/*
==========================================
========================================== 
xxxx. Main Body Styles
==========================================
==========================================
*/


html {
	height: 100%;}

body {
  	background-color: #f3f1ea;
  	color: #150e08;
 	font-size: 14px;
  	font-family: $font_C;
  	font-size: 14px;	
  	line-height: 1.43;
	min-width: 320px;
	-webkit-text-size-adjust: 100%; 
	margin: 0;
	width: 100%;}

a {
	text-decoration: underline;}

h1 {
	letter-spacing: 3px;
	line-height: 36px;
	margin: 0 0 17px;
	text-transform: uppercase;
	word-spacing: -3px;}

h2 {
	letter-spacing: 12px;
	margin: 0 0 42px;}

h3 {
	letter-spacing: 4px;
	margin: 0 0 13px;}

.pagecontent,
.pagecustomer {
	padding: 50px 0;}

.errors {
	line-height: 1;
	padding: 14px 10px 1px;}	

.tooltip {
	font-family: $font_C;}	
.popover{
	font-family: $font_C;}

/*
==========================================
========================================== 
xxxx. Header
==========================================
==========================================
*/

#header {
	padding-top: 60px;
	position: relative;
	z-index: 100;}

#header .row > div {
	position: static;}

#header .line {
	background: #e8e5de;
	left: 0;
	margin-bottom: 16px;
	position: relative;
	position: absolute;
	top: 0;	
	width: 100%;
	z-index: 2;}

#header .line .container-fluid {
	max-width: 1007px;
	position: relative;}

#header .line form {
	display: inline-block;
	vertical-align: top;}

#header .logo {
	width: 194px;
	margin: 0 auto 13px;}

#header .logo img {
	display: block;
	width: 100%;
	height: auto;}

#header .cart {
	margin: 7px 19px -61px 21px;}


/*
==========================================
========================================== 
xxxx. adsfasfasdfafasdf
==========================================
==========================================
*/	

.cycle-pagination ul,
.slideshow .slideshow-pagination ul,
.product-list {
	list-style: none;
	margin: 0;
	padding: 0;}

.clearfix:after,
#header:after,
#header .line:after,
.input-opener:after,
.product-carousel.gallery-block .mask:after,
.product-carousel .mask:after,
.cart-heading:after,
.tables-block:after {
	display: block;
	clear: both;
	content: "";}

.ellipsis {
	overflow: hidden;
	text-overflow: ellipsis; /* 2 */
	white-space: nowrap; /* 1 */}


#wrapper {
	display: table;
	height: 100%;
	min-height: 100vh;
	overflow: hidden;	
	position: relative;
	table-layout: fixed;
	width: 100%;}

#wrapper div.w1 {
	display: table-cell;}



.container-fluid {
	max-width: 1204px;
	margin: 0 auto;
	padding: 0 15px;}





/*
==========================================
========================================== 
xxxx. Form Styles
==========================================
==========================================
*/


.input-box {
	max-width: 194px;}

.input-box .form-control {
	border: 0;}

.input-box i {
	font-size: 18px;}

.input-opener {
	background: #e8e5de;	
	display: block;
	font: 11px $font_B;
	height: 32px;
	letter-spacing: 2px;
	min-width: 194px;	
	padding: 7px 10px 3px 17px;
	text-transform: uppercase;
	text-decoration: none;
	-webkit-transition: background .2s linear;
	transition: background .2s linear;}

.input-opener span span {
	display: inline-block;	
	margin-top: 2px;
	vertical-align: top;}

.input-opener i {
	float: right;
	margin-left: 10px;}

.input-opener .open-text {
	display: block;}

.input-opener .close-text {
	display: none;}

.input-opener .close-text i {
	background: #f3f1ea;	
	border-radius: 100%;
	color: #150e08;
	font-size: 9px;
	height: 17px;
	margin-right: 2px;
	padding: 4px 1px 1px 3px;
	width: 17px;
	text-align: center;}

.input-box .collapse,
.input-box .collapsing {
	background: #150e08;	
	display: block;
	display: block;
	left: 16px;	
	margin-top: 1px;
	overflow: hidden;
	padding: 0 10px;
	position: absolute;
	right: 112px;
	top: 100%;	
	z-index: 1;}

.input-box .collapse .form-control,
.input-box .collapsing .form-control {
	background: #f3f1ea;
	border-radius: 0;
	display: block;
	font-size: 18px;
	font-weight: normal;
	height: 70px;	
	margin: 10px 0;
	padding: 0 21px;	
	text-align: left;
	width: 100%;}

.dropdown-menu {
	border-width: 0 0 1px !important;
	border-radius: 0;	
	left: 16px;
	letter-spacing: 4px;
	min-width: 320px;
	padding: 4px 0 0;}

.dropdown-menu.dropdown-right {
	left: auto;
	right: 16px;}

.dropdown-menu > li > a {
	padding: 11px 16px 12px;}

.dropdown-menu .divider {
	margin: 0;}

textarea.form-control {
	height: 141px;
	resize: none;
	padding-top: 10px;
	padding-bottom: 10px;
	max-width: 600px;
}	

.input-group .form-control {
	background: rgba(21, 14, 8, .06);
	font: 11px $font_B;
	letter-spacing: 2px;
	padding: 0 18px;
	word-spacing: 2px;}

.input-group .btn {
	background: rgba(21, 14, 8, .06);
	border: 0;
	color: #150e08;
	font: 700 15px/1 $font_A; 
	
	/* 
	"amatic_scregular", Menlo, Monaco, Consolas, "Courier New", monospace;
	*/
	
	margin: 0 !important;
	padding-bottom: 0;
	padding-top: 0;
	position: relative;
	transition: color .2s linear;
	-webkit-transition: color .2s linear;}

.input-group .btn:hover {
	color: #7b7a76;}

.input-group .btn:hover:after {
	border-color: #7b7a76;}

.input-group-btn {
	vertical-align: top;
}	

/*
==========================================
========================================== 
xxxx. Nav bar Styles
==========================================
==========================================
*/

.navbar-collapse.in {
	height: 100vh;
	overflow-y: auto;}


.nav-pills {
	font: 11px $font_B;
	letter-spacing: 2px;
	padding: 9px 0;}

.nav-pills li + li:before {
	display: inline-block;
	content: "/";	
	margin: 0 7px;
	vertical-align: top;}

.nav-pills li a {
	display: inline-block;
	padding: 0;
	text-decoration: none;
	vertical-align: top;}

.nav-pills li a:hover {
	background: none;
	text-decoration: underline;}

.navbar-default {
	font-family: $font_B;
	font-weight: 700;
	text-transform: uppercase;}

.navbar-default .navbar-nav {
	display: table;
	float: none;
	padding: 0 5.54%;
	width: 100%;}

.navbar-default .navbar-nav > li {
	display: table-cell;
	float: none;
	letter-spacing: 5px;
	padding: 0 16px;
	text-align: center;
	text-indent: 3px;}

.navbar-default a {
	text-decoration: none;
	transition: color .2s linear, background .2s linear;
	-webkit-transition: color .2s linear, background .2s linear;}

.navbar-default a:hover {
	background: #150e08;
	color: #f3f1ea;}

.navbar-toggle {
	font-size: 7px;
	height: 51px;
	line-height: 15px;
	margin: 0;
	padding: 13px 10px 5px;
	text-transform: uppercase;}

.navbar-toggle .icon-bar {
	box-shadow: 0 0 1px 0 #b7b5af;
	height: 2px;
	margin: 0 auto;
	width: 22px;}

.navbar-toggle .icon-bar + .icon-bar {
	margin-top: 5px;}



/*
==========================================
========================================== 
xxxx. Cart Styles
==========================================
==========================================
*/

.cart {
	display: inline-block;
	letter-spacing: 3px;
	font: 11px $font_B;
	text-decoration: none;
	transition: color .2s linear;	
	vertical-align: top;
	-webkit-transition: color .2s linear;}

.cart .num {
	border-radius: 100%;
	border: 2px solid #150e08;
	background: #f3f1ea;
	display: block;
	font-size: 30px;
	height: 57px;
	letter-spacing: 0;
	margin-bottom: 5px;
	padding: 8px;
	transition: border-color .2s linear;
	-webkit-transition: border-color .2s linear;
	width: 57px;}

.cart:hover {
	color: #94928d;}

.cart:hover .num {
	border-color: #94928d;}

.cart-form {
	border-top: 1px solid white;
	padding-top: 20px;
	padding-bottom: 130px;}

.cart-heading {
	padding: 9px 0 36px;}

.cart-heading h2 {
	font-size: 20px;
	font-weight: 700;
	letter-spacing: 9px;
	line-height: 24px;
	width: 35%;
	word-spacing: 2px;}

.cart-heading .btn-list {
	margin: 5px 5.6% 0 10px;
	max-width: 60%;}

.cart-heading .btn-list li {
	float: right;}

.cart-table {
	margin-right: 10px;
	width: 70.3%;}

.info-text {
	color: #da5b4e;
	font-weight: 700;
	text-transform: uppercase;}

.variants .info-text {
	padding-top: 25px;}

/*
==========================================
========================================== 
xxxx. Welcome Styles
==========================================
==========================================
*/

.w1 {
	height: 100%;
	-webkit-transition: -webkit-transform .3s linear;
	transition: transform .3s linear;
}

.welcome {
	border-bottom: 1px solid rgba(255, 255, 255, .6);
	color: #f3f1ea;	
	font: 14px $font_B;
	padding: 18px 27px;
}

.welcome a,
.welcome span {
	color: #f3f1ea;
	position: relative;}

.welcome a:after,
.welcome span:after {
	content: " >X>";
	display: inline-block;
	margin-left: 4px;
	vertical-align: top;}

.welcome a:hover {
	text-decoration: underline;}


/*
==========================================
========================================== 
xxxx. Carousel Styles
==========================================
==========================================
*/

.carousel,
.cycle-gallery {
	margin-bottom: 93px;}

.carousel-inner .item,
.cycle-gallery .item {
	overflow: hidden;}

.carousel-inner img,
.cycle-gallery img {
	display: block;
	height: auto;
	width: 100%;}

.cycle-gallery {
	position: relative;}

.cycle-gallery .mask {
	overflow: hidden;
	position: relative;}

.cycle-gallery .slideset {
	float: left;
	width: 99999px;}

.cycle-gallery .slide {
	float: left;
	overflow: hidden;
	position: relative;}

.cycle-gallery .slide.active {
	z-index: 1;}

.carousel-caption {
	bottom: 50%;
	font-family: $font_B;	
	left: 15%;
	line-height: 1;
	right: 15%;
	text-shadow: none;
	-ms-transform: translateY(50%);
	-webkit-transform: translateY(50%);
	transform: translateY(50%);}

.carousel-caption.text-right {
	text-align: right;}

.carousel-caption.text-left {
	text-align: left;}

.carousel-caption.black {
	color: black;}

.carousel-caption .title {
	font-weight: 700;
	font-size: 69px;
	display: block;
	margin-bottom: 7px;}

.carousel-caption .sub-title {
	font-size: 20px;
	font-weight: normal;
	margin-bottom: 8px;}

.carousel-caption a {
	color: inherit;
	text-decoration: none;}

.carousel-caption a:hover {
	text-decoration: underline;}

.carousel-control {
	background: none !important;
	bottom: auto;
	cursor: pointer;
	font: 128px/1 $font_A;
	left: 31px;
	opacity: 1;
	text-shadow: none;
	text-decoration: none;
	top: 50%;
	width: auto;
	transform: translateY(-50%);
	transition: color .3s linear;	
	-ms-transform: translateY(-50%);
	-webkit-transform: translateY(-50%);
	-webkit-transition: color .3s linear;
	z-index: 10;}

.carousel-control.right,
.carousel-control.btn-next {
	left: auto;
	right: 31px;}

.carousel-control:hover {
	color: #7b7a76;
	opacity: 1;}

.cycle-pagination,
.carousel-indicators {
	bottom: 0;
	margin-bottom: 4px;}

.cycle-pagination li,
.carousel-indicators li {
	background: white;
	border-width: 2px;
	height: 18px !important;
	margin: 0 1px !important;
	width: 18px !important;
	transition: background .3s linear;
	-webkit-transition: background .3s linear;}

.cycle-pagination li:hover,
.carousel-indicators li:hover {
	background: #7b7a76;}

.cycle-pagination {
	bottom: 0;
	left: 50%;
	position: absolute;
	transform: translateX(-50%);
	-webkit-transform: translateX(-50%);
	-ms-transform: translateX(-50%);
	z-index: 10;}

.cycle-pagination li {
	border-radius: 100%;
	border: 2px solid #1c150f;
	display: inline-block;
	margin: 0 3px 6px !important;
	vertical-align: top;}

.cycle-pagination li.active,
.cycle-pagination li:hover {
	background: #7b7a76;}

.cycle-pagination a {
	display: block;
	overflow: hidden;
	text-indent: 101%;
	white-space: nowrap;}



/*
==========================================
========================================== 
xxxx. Product Styles
==========================================
==========================================
*/

.product-carousel {
	margin-bottom: 109px;
	position: relative;}

.product-carousel .mask {
	margin: 0 96px 43px;
	position: relative;
	overflow: hidden;}

.product-carousel .mask.slick-mask {
	position: static;}

.product-carousel .slideset {
	width: 100%;
	width: 99999px;
	float: left;}

.product-carousel .slideset.slick-gallery {
	width: 100%;}

.product-carousel .slide {
	float: left;
	padding: 16px 3px;
	width: 246px;}

.product-carousel img {
	display: block;
	height: auto;
	width: 100%;}

.product-carousel .btn-prev,
.product-carousel .btn-next {
	border-radius: 100%;
	border: 2px solid #150e08;
	color: #150e08;
	cursor: pointer;
	font: 700 35px/1 $font_A;
	height: 50px;
	left: 24px;
	margin-top: -13px;
	padding: 7px;
	position: absolute;
	text-align: center;
	text-decoration: none;	
	top: 50%;
	width: 50px;
	transform: translateY(-50%);
	transition: color .2s linear, border-color .2s linear;	
	-webkit-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	-webkit-transition: color .2s linear, border-color .2s linear;
	z-index: 100;}

.product-carousel .btn-prev:hover,
.product-carousel .btn-next:hover {
	border-color: #7b7a76;
	color: #7b7a76;}

.mac .product-carousel .btn-prev,
.mac
    .product-carousel .btn-next {
	padding-top: 4px;}

.mac.firefox .product-carousel .btn-prev,
.mac.firefox
    .product-carousel .btn-next {
	padding-top: 7px;}

.product-carousel .btn-next {
	left: auto;
	right: 24px;}


/*
==========================================
========================================== 
xxxx. Big link Styles
==========================================
==========================================
*/


.big-link {
	font: 700 20px $font_B;
	letter-spacing: 6px;
	padding-bottom: 10px;}

.big-link a {
	color: #150e08;
	text-decoration: none;}

.big-link a:hover {
	text-decoration: underline;}



/*
==========================================
========================================== 
xxxx. Category Styles
==========================================
==========================================
*/

.category-box {
	color: white;
	display: block;
	font: 24px/28px $font_B;
	margin-bottom: 2px;
	text-decoration: none;}

.category-box:hover {
	color: white;}

.category-box img {
	display: block;
	height: auto;
	width: 100%;}

.category-box .category-text {
	bottom: 10px;
	left: 15.2%;
	position: absolute;
	right: 10px;
	top: 32.3%;
	z-index: 1;}

.category-box .category-text.full {
	bottom: auto;
	top: 50%;
	left: 12.5%;
	right: 12.5%;
	transform: translateY(-50%);
	-webkit-transform: translateY(-50%);
	-ms-transform: translateY(-50%);}

.category-box .category-text.full .title {
	line-height: 92px;}

.category-box .title {
	display: block;
	font: 72px/74px $font_A;
	letter-spacing: -3px;
	margin-bottom: 1px;}

.category-box .bottom-link {
	bottom: 17px;
	font-size: 19px;
	left: 0;
	letter-spacing: 9px;
	position: absolute;
	right: 0;
	z-index: 1;}

.category-box p {
	letter-spacing: 11px;
	margin: 0 5px 10px;}



/*
==========================================
========================================== 
xxxx. Product Image Styles
==========================================
==========================================
*/


.image-bg,
.image-bg-light {
	display: block;
	position: relative;}

.image-bg:after,
.image-bg-light:after {
	background: rgba(70, 83, 0, .41);
	bottom: 0;
	content: "";	
	left: 0;
	position: absolute;
	right: 0;
	top: 0;	
	transition: opacity .3s linear;
	-webkit-transition: opacity .3s linear;}

.image-bg:hover:after,
.image-bg-light:hover:after {
	opacity: 0;}

.image-bg-light:after {
	background: rgba(70, 83, 0, .13);}



/*
==========================================
========================================== 
xxxx. Section Styles
==========================================
==========================================
*/


section {
	margin-bottom: 121px;}

section:last-child {
	margin-bottom: 115px;}



/*
==========================================
========================================== 
xxxx. Blog Styles
==========================================
========================================== 
*/


.blog-box img {
	display: block;
	height: auto;
	margin-bottom: 15px;
	width: 100%;}

.blog-box a {
	color: inherit;
	text-decoration: none;}

.blog-box a:hover {
	text-decoration: underline;}

.blog-text {
	padding: 0 17% 0 0;}

.more {
	font: 41px $font_A;
	letter-spacing: -1px;
	text-decoration: none;}

.more:hover {
	text-decoration: underline;}


/*
==========================================
========================================== 
xxxx. Slideshow Styles
==========================================
==========================================
*/


.slideshow {
	position: relative;}

.slideshow .slide {
	left: 0;
	position: absolute;
	top: 0;
	width: 100%;
	z-index: 1;}

.slideshow .slide.active {
	z-index: 2;}

.slideshow .slideshow-pagination {
	margin: 18px -10px 10px;
	position: relative;
	z-index: 3;}

.slideshow .slideshow-pagination ul {
	width: 100%;}

.slideshow .slideshow-pagination li {
	box-sizing: border-box;
	display: inline-block;
	padding: 0 11px 11px;
	margin-right: -8px;
	vertical-align: middle;
	width: 20%;}

.slideshow .slideshow-pagination img {
	max-width: 102px;}

.slideshow img {
	display: block;
	height: auto;
	width: 100%;}

.product-form .slideshow {
	margin-top: 3px;
	margin-right: -14px;}




/*
==========================================
========================================== 
xxxx. Page Customer Styles
==========================================
==========================================
*/


.pagecustomer input.btn,
.btn {
	padding: 15px 24px 13px;
	text-transform: uppercase;
	text-align: center;}

.pagecustomer input.btn[type="submit"],
.btn[type="submit"] {
	height: 50px;
	padding: 5px 10px;}

.pagecustomer input.btn.input-sm,
.btn.input-sm {
	height: 32px;
	padding: 5px 10px;}

#header .pagecustomer input.btn.input-sm,
#header
    .btn.input-sm {
