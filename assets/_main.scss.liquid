	.btn-list {
		float: none !important;
		max-width: none !important; }

	.btn-list li {
		display: block;
		margin-bottom: 15px; }

	.btn-list .btn {
		display: block;
		letter-spacing: 4px;
		max-width: 400px;
		margin: 0 auto;
		padding: 12px 15px 11px !important;
		width: 100%;	}

	/*
	==========================================
	==========================================
	xxxx. Table (Small Devices)
	==========================================
	==========================================
	*/

	.table {
		display: block;
		font-size: 9px;
		padding: 0 15px 46px;
		text-align: center;	}

	.table tbody,
	.table thead,
	.table tr {
		display: block; }

	.table th {
		display: none;
		font-size: 9px; }

	.table td {
		display: block;
		width: auto !important; }

	.table td.mobile-mini {
		display: inline-block;
		margin-right: -4px;
		vertical-align: top !important;
		width: 20% !important;	}

	.table td.mobile-mini:before {
		padding-bottom: 3px; }

	.table td:before {
		content: attr(data-title);
		display: block;
		font-size: 9px;
		padding-bottom: 9px;
		text-transform: uppercase; }

	.table img {
		display: block;
		width: 100%;
		height: auto;
		max-width: 400px;
		margin: 0 auto; }

	.table .total {
		font-size: 15px;
		margin-top: 10px;
		display: block; }

	.table a.remove {
		font-size: 12px;
		height: 28px;
		line-height: 1;
		margin-top: 2px;
		padding: 6px 3px;
		width: 28px; }

	.table dl {
		padding-bottom: 23px; }

	.table dl dt,
	.table dl dd {
		float: left;
		margin-bottom: -1px;
		text-align: right;
		width: 48%;	}

	.table dl dd {
		text-align: left; }

	.table .mobile-margin {
		padding-bottom: 38px; }



	/*
	==========================================
	==========================================
	xxxx. Order Summary in Cart (Small Devices)
	==========================================
	==========================================
	*/

	.total-block {
		font-size: 9px;
		padding: 0 25px;	}

	.total-block .title {
		font-size: 15px;
		letter-spacing: 1px;
		margin-bottom: 15px;}

	.total-block .cost {
		font-size: 21px;
		letter-spacing: 1px;
		margin-bottom: 18px;	}

	.total-block label {
		letter-spacing: 0;
		margin-bottom: 3px; }

	.total-block .btn-label {
		font-size: 15px;
		letter-spacing: 1px;
		margin-bottom: 5px;	}

	.total-block .form-group {
		margin-bottom: 14px; }

	.total-block .sub-total {
		padding-bottom: 8px; }

	.total-block .sub-total .title {
		font-size: 9px;
		letter-spacing: 0;
		margin-bottom: 4px;	}

	.total-block .btn {
		font-size: 11px;
		display: block;
		letter-spacing: 4px;
		max-width: 400px;
		padding: 12px 15px 11px !important;
		width: 100%;	}


	/*
	==========================================
	==========================================
	xxxx. Form (Small Devices)
	==========================================
	==========================================
	*/

	textarea.form-control {
		height: 100px; }


	.mce_inline_error + .mce_inline_error {
		bottom: 0;
		left: 50%;
		position: absolute;
		max-width: 400px;
		transform: translateX(-50%);
		-webkit-transform: translateX(-50%);
		-ms-transform: translateX(-50%);
	}

	/*
	==========================================
	==========================================
	xxxx. My Account (Small Devices)
	==========================================
	==========================================
	*/

	#customer_orders,
	#customer_sidebar {
		float: none !important;
		padding: 0 0 20px !important;
		max-width: 400px;
		margin: 0 auto;
		text-align: center  !important;
		width: auto !important; }

	/*
	==========================================
	==========================================
	xxxx. NavBar (Small Devices)
	==========================================
	==========================================
	*/
  	.navbar-default .navbar-collapse,
    .navbar-default .navbar-form {
      height: auto !important;
      padding-bottom: 99999px;
      margin: 0 0 -99999px !important;}

}

/* 
Small devices (smart phones, browser window is smaller than 480px) 
*/
@media (max-width: $small) {

	.carousel-inner img,
	.cycle-gallery img {
		height: 187px !important;
		left: 50%;
		max-width: none !important;
		position: relative;
		top: 50%;
		width: auto !important;
		-webkit-transform: translateX(-50%);
		-ms-transform: translateX(-50%);
		transform: translateX(-50%); }

	.product-form .slideshow {
		margin: 0 -5px; }

	.table td.mobile-mini {
		display: inline-block;
		margin-right: -4px;
		vertical-align: top !important;
		width: 33.33% !important;}
}
